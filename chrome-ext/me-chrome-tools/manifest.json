{
  "manifest_version": 3,
  "name": "INI TOOLS GWA!!!",
  "version": "1.0",
  "description": "Ekstensi Chrome untuk alat bantu sederhana dalam pekerjaan sehari-hari.",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "service_worker": "bg.js",
    "type": "module"
  }
}
